                <xpath>Defs/ThingDef[defName="Milira_ArtemisArmor"]</xpath>
                <value>
                    <statBases>
			            <Insulation_Cold>100</Insulation_Cold>
                    </statBases>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Milira_ArtemisArmor"]/equippedStatOffsets</xpath>
                <value>
                    <DecompressionResistance>0.75</DecompressionResistance>
                    <VacuumSpeedMultiplier>4</VacuumSpeedMultiplier>
                </value>
            </li>
            <!--米莉拉阿忒弥斯战甲（守御） Milira_ArtemisArmorII-->
