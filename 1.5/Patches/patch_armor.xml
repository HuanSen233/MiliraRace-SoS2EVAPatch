                <xpath>Defs/ThingDef[defName="Milira_DaedalusHelmet"]/equippedStatOffsets</xpath>
                <value>
			        <HypoxiaResistance>1</HypoxiaResistance>
                    <DecompressionResistance>0.25</DecompressionResistance>
                </value>
            </li>
            <!--米莉拉代达罗斯战甲 Milira_DaedalusArmor-->
            <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="Milira_DaedalusArmor"]/statBases/Insulation_Cold</xpath>
                <value>
			        <Insulation_Cold>100</Insulation_Cold>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Milira_DaedalusArmor"]/equippedStatOffsets</xpath>
                <value>
                    <DecompressionResistance>0.75</DecompressionResistance>
                    <VacuumSpeedMultiplier>4</VacuumSpeedMultiplier>
                </value>
            </li>
            <!--米莉拉代达罗斯之翼 Milira_DaedalusWings-->
