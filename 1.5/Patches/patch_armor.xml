<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!--护甲 armor-->
	<Operation Class="PatchOperationSequence">
        <operations>
            <!--瓦尔基里 Valkyr=================================================================-->
            <!--米莉拉瓦尔基里战盔 Milira_ValkyrHelmet-->
            <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="Milira_ValkyrHelmet"]/statBases/Insulation_Cold</xpath>
                <value>
			        <Insulation_Cold>50</Insulation_Cold>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Milira_ValkyrHelmet"]/equippedStatOffsets</xpath>
                <value>
			        <HypoxiaResistance>1</HypoxiaResistance>
                    <DecompressionResistance>0.25</DecompressionResistance>
                </value>
            </li>
            <!--米莉拉瓦尔基里战甲 Milira_ValkyrArmor-->
            <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="Milira_ValkyrArmor"]/statBases/Insulation_Cold</xpath>
                <value>
			        <Insulation_Cold>100</Insulation_Cold>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Milira_ValkyrArmor"]/equippedStatOffsets</xpath>
                <value>
                    <DecompressionResistance>0.75</DecompressionResistance>
                    <VacuumSpeedMultiplier>5</VacuumSpeedMultiplier>
                </value>
            </li>

            <!--阿忒弥斯 Artemis=================================================================-->
            <!--米莉拉阿忒弥斯之眼 Milira_ArtemisHelmet-->
