                <xpath>Defs/ThingDef[defName="Milira_ArtemisArmorII"]</xpath>
                <value>
                    <statBases>
			            <Insulation_Cold>100</Insulation_Cold>
                    </statBases>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Milira_ArtemisArmorII"]/equippedStatOffsets</xpath>
                <value>
                    <DecompressionResistance>0.75</DecompressionResistance>
                    <VacuumSpeedMultiplier>4</VacuumSpeedMultiplier>
                </value>
            </li>

            <!--代达罗斯 Daedalus=================================================================-->
            <!--米莉拉代达罗斯之冠 Milira_DaedalusHelmet-->
            <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="Milira_DaedalusHelmet"]/statBases/Insulation_Cold</xpath>
                <value>
			        <Insulation_Cold>50</Insulation_Cold>
